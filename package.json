  },
  "author": "Romain DUBOC <romain@duboc.pro>",
  "engines": {
    "homebridge": ">=1.0.0"
  },
  "keywords": [
    "homebridge-plugin"
  ],
  "license": "ISC",
  "files": [
    "LICENSE",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dubocr/homebridge-sun-protect.git"
  },
  "bugs": {
    "url": "https://github.com/dubocr/homebridge-sun-protect.git/issues"
  },
  "devDependencies": {
    "@types/node": "10.17.19",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "eslint": "^7.17.0",
    "homebridge": "^1.1.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/suncalc": "^1.8.0",
    "suncalc": "^1.8.0"
